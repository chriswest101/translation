{
    "name": "chriswest101/translation",
    "description": "A Translation package for Laravel 5 with database and cache support",
    "keywords": [
        "laravel-translator",
        "laravel",
        "translator",
        "translation",
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris West"
        }
    ],
    "require": {
        "laravel/framework": "~6.0|~7.0|^8.0",
        "doctrine/dbal": "^2.5|^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "~4.0|^6.25",
        "mockery/mockery": "^1.2.3"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\Translation\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
